# Contract Deployment Parameters
# -------------------------

# Required Parameters
# ------------------
OWNER_ADDRESS= 0x0000000000000000000000000000000000000000  # The address that will own the contract
ADMIN_ADDRESS= 0x0000000000000000000000000000000000000000  # The address that will have admin privileges

ORACLE_ROUTER= 0x0000000000000000000000000000000000000000  # Chainlink Functions Router address for your network
ORACLE_DON_ID=   # Chainlink DON ID for your network

# Optional Parameters (with defaults)
# ---------------------------------
MIN_BET_AMOUNT=0.01     # Minimum bet amount in ETH (default: 0.01 ETH)
HOUSE_FEE=100          # House fee in basis points (default: 100 = 1%)
ROUND_MASTER_FEE=200   # Round master fee in basis points (default: 200 = 2%)
ORACLE_CALLBACK_GAS_LIMIT=300000  # Gas limit for oracle callback (default: 300000)

# Network Configuration
# -------------------

# Your wallet's private key for deployment
ACCOUNT_PRIVATE_KEY= xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Your Etherscan API key for contract verification
ETHERSCAN_API_KEY=  xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx